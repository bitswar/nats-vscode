{
  "name": "nats-vscode",
  "displayName": "nats-vscode",
  "description": "Nats client from vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "repository": {
    "url": "https://github.com/bitswar/nats-vscode"
  },
  "categories": [
    "Other"
  ],
  "publisher": "bitswar",
  "activationEvents": [
      "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nats.connect",
        "title": "Connect to NATS server"
      },
      {
        "command": "nats.disconnect",
        "title": "Disconnect from NATS server"
      },
      {
        "command": "nats.clearSavedConnection",
        "title": "Clear saved NATS connection"
      }
    ],
    "languages": [
      {
        "id": "nats",
        "extensions": [
          ".nats"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "publish": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@vscode/vsce": "^3.3.1",
    "nats": "^2.29.3"
  }
}